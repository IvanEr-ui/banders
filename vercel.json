{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "dependencies": {
    "@vercel/node": "^2.15.3",
    "bootstrap": "^5.3.0",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "jquery": "^3.7.0",
    "mongodb": "^5.6.0",
    "mongoose": "^7.2.2",
    "puppeteer": "^20.5.0"
  }
}
